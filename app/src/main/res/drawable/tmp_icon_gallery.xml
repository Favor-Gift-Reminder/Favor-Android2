<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/line2" />
            <corners android:radius="500dp" />
            <padding
                android:bottom="14dp"
                android:left="14dp"
                android:right="14dp"
                android:top="14dp" />
            <stroke
                android:width="4dp"
                android:color="@color/white" />
        </shape>
    </item>
    <item android:drawable="@drawable/icon_default_gallery" />
</layer-list>